{
  "name": "@olton/hooks",
  "version": "0.8.0",
  "description": "The set of js hooks.",
  "type": "module",
  "main": "dist/hooks.esm.js",
  "exports": {
    "types": "./dist/types/hooks.d.ts",
    "require": "./dist/hooks.cjs.js",
    "import": "./dist/hooks.esm.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist/* lib/*",
    "build": "npm run clean && cross-env NODE_ENV=production rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c",
    "watch": "cross-env NODE_ENV=development rollup -c -w",
    "pub": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "biome lint --write ./src"
  },
  "keywords": [
    "js",
    "javascript",
    "state",
    "event",
    "memo",
    "debounce",
    "throttle",
    "pipe",
    "compose",
    "curry"
  ],
  "author": {
    "name": "Serhii Pimenov",
    "email": "serhii@pimenov.com.ua"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "cross-env": "^7.0.3",
    "rollup": "^4.18.0",
    "rollup-plugin-progress": "^1.1.2",
    "shx": "^0.3.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  }
}
