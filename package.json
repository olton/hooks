{
  "name": "@olton/hooks",
  "version": "0.3.0",
  "description": "The set of js hooks.",
  "type": "module",
  "main": "dist/hooks.cjs.js",
  "module": "dist/hooks.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist/* lib/*",
    "build": "npm run clean && cross-env NODE_ENV=production rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c",
    "watch": "cross-env NODE_ENV=development rollup -c -w",
    "pub": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "js",
    "javascript",
    "usestate",
    "useevent",
    "useeffect"
  ],
  "author": {
    "name": "Serhii Pimenov",
    "email": "serhii@pimenov.com.ua"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.5",
    "cross-env": "^7.0.3",
    "rollup": "^4.15.0",
    "shx": "^0.3.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-progress": "^1.1.2"
  }
}
